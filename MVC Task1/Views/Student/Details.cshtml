@model Student


<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="/Images/@Model.ImagePath" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title">Name: @Model.Name</h5>
        <p class="card-text"> <strong>Id:</strong> @Model.Id </p>
        <p class="card-text"> <strong>Address:</strong> @Model.Address</p>
        <a href="/Student/GetAllStudents" class="btn btn-primary">Student List</a>
    </div>
</div>



